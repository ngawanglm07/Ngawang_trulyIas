<%- include("partials/header"); -%>
<% posts.forEach(function(post){ %>
    <div class="box ">
     <div class="inside-box">
     <h1 style="font-size:22px; font-weight: bold;"><%= post.title %></h1>
     <h2><%= post.question %></h2>
     <h5 class="answer"><%= post.answer %></h5>
     <h5><%= post.url %></h5>
     <h5><%= post.submitTime %></h5>
     <h5><%= post.subject %></h5>
     <h5><%= post.topic %></h5>
    </div>
    </div>
    <% }) %> 
<%- include("partials/footer"); -%>